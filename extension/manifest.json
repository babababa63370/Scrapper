{
  "manifest_version": 2,
  "name": "Web Scraper",
  "version": "1.0",
  "description": "Scrape HTML content from current page",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  }
}
